Ответ:
public class HW_6_1_8_Circle {

    public static void main(String[] args) {
        int radius = 20;
        int padding = 3;

        int max = radius + padding;
        int radiusSq = radius * radius;

        for (int y = max; y >= -max; y--) {
            for (int x = -max; x <= max; x++) {
                if (x*x + y*y > radiusSq) {
                    System.out.print("..");
                }else {
                    System.out.print("MM");
                }

            }
            System.out.println();
        }

    }
}
